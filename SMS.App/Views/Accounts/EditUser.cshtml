@model SMS.App.ViewModels.AdministrationVM.EditUserVM
@{
}
<form asp-action="EditUser" asp-controller="Accounts" method="post">
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="UserName"></label>
        <input asp-for="UserName" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="NormalizedEmail"></label>
        <input asp-for="NormalizedEmail" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="EmailConfirmed"></label>
        <input asp-for="EmailConfirmed" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="PhoneNumberConfirmed"></label>
        <input asp-for="PhoneNumberConfirmed" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="UserType"></label>
        <input asp-for="UserType" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <label class="col-md-2 text-md-right" asp-for="ReferenceId"></label>
        <input asp-for="ReferenceId" class="form-control" />
    </div>
    <div class="form-group form-inline">
        <div class="col-md-4 offset-md-2">
            <input type="submit" value="Submit" class="btn btn-info" /> 
            <a asp-action="UserList" class="btn btn-success">Back To List</a>
        </div>
    </div>
</form>