@model IEnumerable<SMS.Entities.AcademicSubjectType>

@{
    ViewData["Title"] = "Academic Subject Type";
    GlobalUI.PageTitle = ViewBag.Title;
    int sl = 0;
}


<table class="table table-responsive-sm">
    <thead>
        <tr>
            <th>#</th>
            <th>
                @Html.DisplayNameFor(model => model.SubjectTypeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>

            <th>
                <partial name="_Create" />
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@(++sl).</td>
            <td>
                @Html.DisplayFor(modelItem => item.SubjectTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>

            <td>
                <partial name="_Edit_Details_Delete" model="@item.Id" />
            </td>
        </tr>
}
    </tbody>
</table>
