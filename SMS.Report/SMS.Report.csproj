<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ReportingServices.ReportViewerControl.WebForms" Version="150.1537.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="App_Code\myDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>myDataSet.xsd</DependentUpon>
    </Compile>
    <Compile Update="dsAdmitCard.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>dsAdmitCard.xsd</DependentUpon>
    </Compile>
    <Compile Update="DS\rptAttendanceDS.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>rptAttendanceDS.xsd</DependentUpon>
    </Compile>
    <Compile Update="DS\rptStudentDS.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>rptStudentDS.xsd</DependentUpon>
    </Compile>
    <Compile Update="DS\rptStudentPaymentFullInfo.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>rptStudentPaymentFullInfo.xsd</DependentUpon>
    </Compile>
    <Compile Update="Rpt_Academic_Exam.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Rpt_Academic_Exam.xsd</DependentUpon>
    </Compile>
    <Compile Update="SMSDBDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SMSDBDataSet.xsd</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="App_Code\myDataSet.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>myDataSet.Designer.cs</LastGenOutput>
    </None>
    <None Update="dsAdmitCard.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>dsAdmitCard.Designer.cs</LastGenOutput>
    </None>
    <None Update="DS\rptAttendanceDS.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>rptAttendanceDS.Designer.cs</LastGenOutput>
    </None>
    <None Update="DS\rptStudentDS.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>rptStudentDS.Designer.cs</LastGenOutput>
    </None>
    <None Update="DS\rptStudentPaymentFullInfo.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>rptStudentPaymentFullInfo.Designer.cs</LastGenOutput>
    </None>
    <None Update="Reports\rptStudent.rdlc">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Rpt_Academic_Exam.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>Rpt_Academic_Exam.Designer.cs</LastGenOutput>
    </None>
    <None Update="SMSDBDataSet.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>SMSDBDataSet.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>